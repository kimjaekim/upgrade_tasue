# React app environment variables
# Copy to .env in this folder and set your values

# Base URL of the backend proxy server
REACT_APP_API_BASE=http://localhost:3001

# Kakao Maps JavaScript API key
REACT_APP_KAKAO_APP_KEY=YOUR_KAKAO_APP_KEY

# Tashu Open API token (used by the backend /station endpoint if proxied client-side)
REACT_APP_TASHU_TOKEN=YOUR_TASHU_TOKEN
